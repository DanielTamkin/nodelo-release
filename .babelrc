{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        },
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-classes", {
        "loose": false
      }
    ],
    [
      "@babel/plugin-proposal-class-properties", { "loose": true }
    ],
    [
      "@babel/plugin-transform-runtime", {
        "corejs": 3,
        "regenerator": true,
      }
    ]
  ]
}
